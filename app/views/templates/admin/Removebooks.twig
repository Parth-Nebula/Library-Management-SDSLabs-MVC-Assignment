{% extends "templates/0__base.twig" %}
{% block title %}
	{{- parent() -}}
    Remove Books
{% endblock %}
{% block head %}
	{{- parent() -}}
    <link rel="stylesheet" type="text/css" href="/style/admin/Removebooksstyle.css">
{% endblock %}
{% block body %}
	{{- parent() -}}
    <div id="tableDiv">
        <table id="booksTable">
        </table>
    </div>
    <div class="resultBar">
    {{ data }}
    </div>
    <form class="back" action="/goToAdminPortal" method="POST" >
        <input type="submit" class="backButton" value="Back">
    </form>
{% endblock %}
{% block bottom %}
	{{- parent() -}}
    <script>
        data_one = {{ books|json_encode|raw }};
    </script>
    <script src="script/admin/Removebooksscript.js"></script>
{% endblock %}